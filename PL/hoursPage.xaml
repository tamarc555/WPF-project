<Page x:Class="PL.hoursPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PL"
      mc:Ignorable="d" 
      d:DesignHeight="550" d:DesignWidth="800"
      Title="hoursPage">

    <Grid Background="#FFDCF2F0">
        <Button x:Name="connectButton"  HorizontalAlignment="Left" Height="38" Margin="387,480,0,0" VerticalAlignment="Top" Width="110" Background="{x:Null}" BorderBrush="{x:Null}" Click="Button_Click">
            <StackPanel Height="35" Width="100">
                <StackPanel.Background>
                    <ImageBrush ImageSource="/connect4.png"/>
                </StackPanel.Background>
            </StackPanel>
        </Button>
        <Image HorizontalAlignment="Left" Height="383" Margin="48,30,0,0" VerticalAlignment="Top" Width="463" Source="/hours3.png"/>
        <Image HorizontalAlignment="Left" Height="65" Margin="364,410,0,0" VerticalAlignment="Top" Width="190" Source="/connect3.png"/>

    </Grid>
</Page>
